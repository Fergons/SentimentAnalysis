{
    "name": "frontend",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "package": "svelte-kit package",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
        "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
        "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"generate-client": "openapi --input ./openapi.json --output ./src/lib/client --client axios"
    },
    "devDependencies": {
        "@fontsource/fira-mono": "^4.5.10",
        "@material/theme": "^14.0.0",
        "@material/typography": "^14.0.0",
        "@mdi/js": "^7.1.96",
        "@neoconfetti/svelte": "^1.0.0",
        "@playwright/test": "^1.28.1",
        "@smui/banner": "^7.0.0-beta.0",
        "@smui/button": "^7.0.0-beta.0",
        "@smui/card": "^7.0.0-beta.0",
        "@smui/checkbox": "^7.0.0-beta.0",
        "@smui/common": "^7.0.0-beta.0",
        "@smui/drawer": "^7.0.0-beta.0",
        "@smui/form-field": "^7.0.0-beta.0",
        "@smui/icon-button": "^7.0.0-beta.0",
        "@smui/layout-grid": "^7.0.0-beta.0",
        "@smui/list": "^7.0.0-beta.0",
        "@smui/paper": "^7.0.0-beta.0",
        "@smui/snackbar": "^7.0.0-beta.0",
        "@smui/switch": "^7.0.0-beta.0",
        "@smui/textfield": "^7.0.0-beta.0",
        "@smui/tooltip": "^7.0.0-beta.0",
        "@smui/top-app-bar": "^7.0.0-beta.0",
        "@sveltejs/adapter-auto": "^1.0.0",
        "@sveltejs/kit": "^1.0.0",
        "@types/cookie": "^0.5.1",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "simple-icons": "^8.4.0",
        "smui-theme": "^7.0.0-beta.0",
        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "tinygesture": "^2.0.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vitest": "^0.25.3",
        "zod": "^3.20.3"
    },
    "type": "module",
    "dependencies": {
        "axios": "^1.3.2"
    }
}
